{
  "pipeline": {
    "name": "inference"
  },
  "transform": {
    "image": "pachyderm/iris-infer:python",
    "cmd": [
	"python3",
	"/code/pyinfer.py",
	"/pfs/model/",
	"/pfs/attributes/",
	"/pfs/out/"
    ]
  },
  "parallelism_spec": {
    "constant": "1"
  },
  "input": {
    "cross": [
      {
        "pfs": {
	  "repo": "attributes",
	  "glob": "/*"
	}
      },
      {
	"pfs": {
	  "repo": "model",
	  "glob": "/"
	}
      }
    ]
  }
}
